<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DndManager.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DndManager"
    xmlns:pages="clr-namespace:DndManager.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="DndManager">

    <ShellContent ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />
    <ShellContent Title="CharacterCardPage" Route="CharacterCardPage" ContentTemplate="{DataTemplate pages:CharacterCardPage}" />
</Shell>
